print('Hello World')

a=int (input('What is a?'))
b=int (input('What is b?'))


if(a>b):
    print('a is bigger')
    print('Obviously')
else:
    print('b is bigger')
    print('Duhh')

if(a==b):
    print('They are the same')

print('That was exciting...')
